{
    "name": "arayiksmbatyan/laravel-package-hello-world",
    "description": "Laravel Package Hello World",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arayik Smbatyan",
            "email": "arayiksmbatyan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "HelloWorld\\" : "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelloWorld\\Tests\\" : "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelloWorld\\ServiceProvider"
            ],
            "aliases": {
                "HelloWorld": "HelloWorld\\HelloWorldFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.22"
    }
}
